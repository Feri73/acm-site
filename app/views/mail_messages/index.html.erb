<h1>Listing mail_messages</h1>

<table>
  <thead>
    <tr>

      <th>Subject</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @mail_messages.each do |mail_message| %>
      <tr>
        <td><%= mail_message.subject %></td>
        <td><%= link_to 'Show', mail_message %></td>
        <td><%= link_to 'Destroy', mail_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mail message', new_mail_message_path %>
